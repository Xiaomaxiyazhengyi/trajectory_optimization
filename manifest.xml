<package>
  <description brief="Trajectory Optimization">

Trajectory optimization based on the CHOMP motion planner.

  </description>
  <author>Lorenzo Riano / lorenzo.riano@berkeley.edu</author>
  <license>BSD</license>
  <review status="unreviewed" notes=""/>
  <url>http://ros.org/wiki/trajectory_optimization</url>

  <depend package="roscpp" />
  <depend package="planning_models" />
  <depend package="planning_environment" />
  <depend package="arm_navigation_msgs" />
  <depend package="kdl" />
  <depend package="kdl_parser" />
  <depend package="angles" />
  <depend package="visualization_msgs" />
  <depend package="tf" />
  <depend package="distance_field" />
  <depend package="sensor_msgs" />
  <depend package="trajectory_msgs" />
  <depend package="filters" />
  <depend package="spline_smoother" />
  <depend package="collision_proximity"/>
  <depend package="constraint_aware_spline_smoother"/>
  <depend package="common_rosdeps"/>
  
  <depend package="verb_msgs"/>
  <depend package="kinematics_msgs"/>

  <rosdep name="eigen" />

  <export>
    <cpp cflags="-I${prefix}/include -I${prefix}/srv/cpp  -I${prefix}/msg/cpp"/>
  </export>


  <platform os="ubuntu" version="9.04"/>
  <platform os="ubuntu" version="9.10"/>
  <platform os="ubuntu" version="10.04"/>
</package>


